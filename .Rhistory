# Your original data remains untouched
df <- data.frame(x = c(1.2345, NA, 3.6789))
# This only formats the display, not the data
datatable(df) %>%
formatRound('x', digits = 2)
datatable(df) %>%
formatRound(columns = 'numeric_col', digits = 2) %>%
formatStyle(
'numeric_col',
valueColumns = 'numeric_col'
)
datatable(df) %>%
formatRound(columns = 'x', digits = 2) %>%
formatStyle(
'x',
valueColumns = 'x'
)
# Create display version only
df_display <- df
numeric_cols <- sapply(df, is.numeric)
for(col in names(df)[numeric_cols]) {
df_display[[col]] <- ifelse(is.na(df[[col]]),
"NA",
as.character(round(df[[col]], 2)))
}
datatable(df_display)
